{
    "name": "microbit-pxt-blelog",
    "version": "0.1.3",
    "description": "Data Logger Bluetooth Service",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*",
        "datalogger": "*"
    },
    "files": [
        "README.md",
        "blelog.ts",
        "blelog.cpp",
        "shims.d.ts",
        "enums.d.ts",
        "BLELogService.h",
        "BLELogService.cpp",
        "debug.h"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "5.0.12",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "disablesVariants": [
        "mbdal"
    ],
    "authors": [
        "Bill Siever <bsiever@gmail.com>"
    ],
    "card": {
        "name": "Data Logger Bluetooth Service",
        "imageUrl": "file:../icon.png",
        "feedbackUrl": "http://siever.info"
    },
    "yotta": {
        "config": {
            "DMESG_SERIAL_DEBUG": 1,
            "DEVICE_DMESG_BUFFER_SIZE": 8192,
            "microbit-dal": {
                "bluetooth": {
                    "open": 0,
                    "whitelist": 1,
                    "security_level": "SECURITY_MODE_ENCRYPTION_OPEN_LINK",
                    "device_info_service": 1
                }
            }
        }
    }
}
